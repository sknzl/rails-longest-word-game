<div class="wrapper">
  <div class="container text-left">
    <h1>Result</h1>
    <div class="row">
    <div class="score">
      <div class="col-xs-12">
        <p><%=@message%></p>
        <p>Total score: <%=session[:score]%></p>
        <ul class="list-unstyled">
        <li><%= link_to "Reset score", reset_path, method: :get %></li>
        <li><%= link_to "New game", new_url, method: :get %></li>
        </ul>
      </div>
      </div>
    </div>
  </div>
</div>
